Seril.begin (9600);
Pinmode (trigpin, OUTPUT);
Pinmode (echopin, INPUT);
Pinmode (led, OUTPUT);
}
{
DigitalWrite (pingpin, LOW);
Delay Microseconds (2);
DigitalWrite (pingpin, HIGH);
Delay Microseconds (5);
DigitalWrite (pingpin, LOW);
pinmode(pingpin, INPUT);
duration = pulseInhigh;
